{
    "version": 2,
    "builds": [
        {
          "src": "./index.js",
          "use": "@vercel/node"
        }
    ],
    "routes": [      
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "headers": [
      {
        "source": "/images/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "redirects": [
      { "source": "/old", "destination": "/new", "statusCode": 301 }
    ]
  }
  